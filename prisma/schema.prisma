// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output        = "./generated/prisma"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// ============================================
// TABLES PRINCIPALES
// ============================================

model Species {
  id          Int     @id @default(autoincrement())
  speciesFr   String? @map("species_fr") @db.VarChar(255)
  speciesLat  String? @unique @map("species_lat") @db.VarChar(255)
  speciesEn   String? @map("species_en") @db.VarChar(255)
  family      String? @db.VarChar(255)
  genus       String? @db.VarChar(255)
  inFrance    Boolean? @map("in_france")
  inEurope    Boolean? @map("in_europe")

  bibliographies  BibliographySpecies[]
  eurobatraitData      EurobatraitData?
  anageData            AnageData?

  @@map("species")
}

model Bibliography {
  id                          Int      @id @default(autoincrement())
  citation                    String?  @db.Text
  reference                   String?  @db.Text
  publicationDate             String?  @map("publication_date") @db.VarChar(255)
  country                     String?  @db.VarChar(255)
  region                      String?  @db.VarChar(255)
  studyDuration               String?  @map("study_duration") @db.VarChar(255)
  datesOfTheStudy             String?  @map("dates_of_the_study") @db.VarChar(255)
  season                      String?  @db.VarChar(255)
  sampleSize                  Decimal? @map("sample_size") @db.Decimal
  numberMalesSampled          Decimal? @map("number_males_sampled") @db.Decimal
  numberFemalesSampled        Decimal? @map("number_females_sampled") @db.Decimal
  areaSizeHa                  Decimal? @map("area_size_ha") @db.Decimal
  methodologyUsed             String?  @map("methodology_used") @db.Text
  modelUsed                   String?  @map("model_used") @db.Text
  captureMethod               String?  @map("capture_method") @db.Text
  markingMethod               String?  @map("marking_method") @db.Text
  captureFrequency            String?  @map("capture_frequency") @db.Text
  meanRecaptureProbability    Decimal? @map("mean_recapture_probability") @db.Decimal
  sdMeanRecaptureProbability  Decimal? @map("sd_mean_recapture_probability") @db.Decimal
  biasesAndProblems           String?  @map("biases_and_problems") @db.Text
  otherNote                   String?  @map("other_note") @db.Text
  reliability                 String?  @db.VarChar(255)

  species             BibliographySpecies[]

  @@map("bibliography")
}

// Table d'association many-to-many entre Bibliography et Species
// MAINTENANT elle contient aussi les données démographiques spécifiques à la combinaison Bibliography+Species
model BibliographySpecies {
  id             Int      @id @default(autoincrement())
  bibliographyId Int      @map("bibliography_id")
  speciesId      Int      @map("species_id")

  bibliography   Bibliography @relation(fields: [bibliographyId], references: [id], onDelete: Cascade)
  species        Species      @relation(fields: [speciesId], references: [id], onDelete: Cascade)

  // Données démographiques spécifiques à cette combinaison Bibliography+Species
  ageGroups           AgeGroup[]
  dispersalDistances  DispersalDistance[]

  @@unique([bibliographyId, speciesId])
  @@map("bibliography_species")
}

// ============================================
// HIÉRARCHIE DES DONNÉES DÉMOGRAPHIQUES
// ============================================

model AgeGroup {
  id           Int     @id @default(autoincrement())
  ageGroup     String? @map("age_group") @db.VarChar(255)
  consideredAs String? @map("considered_as") @db.VarChar(255)

  /*
  Lié à BibliographySpecies
  Permet de créer des AgeGroup pour une combinaison Bibliography+Species
  */
  bibliographySpeciesId Int?                  @map("bibliography_species_id")
  bibliographySpecies   BibliographySpecies? @relation(fields: [bibliographySpeciesId], references: [id], onDelete: Cascade)

  survivals   Survival[]
  fecundities Fecundity[]

  @@map("age_group")
}

model Survival {
  id                              Int      @id @default(autoincrement())
  ageGroup                        String?  @map("age_group") @db.VarChar(255)
  ageGroupConsideredInPublication String?  @map("age_group_considered_in_the_publication") @db.VarChar(255)
  sex                             String?  @db.VarChar(50)

  // Champs principaux
  minSurvivalRate                 Decimal? @map("min_survival_rate") @db.Decimal
  meanSurvivalRate                Decimal? @map("mean_survival_rate") @db.Decimal
  maxSurvivalRate                 Decimal? @map("max_survival_rate") @db.Decimal
  recaptureProbability            Decimal? @map("recapture_probability") @db.Decimal
  minLongevityYears               Decimal? @map("min_longevity_years") @db.Decimal
  meanLongevityYears              Decimal? @map("mean_longevity_years") @db.Decimal
  maxLongevityYears               Decimal? @map("max_longevity_years") @db.Decimal

  // Données de confiance pour minSurvivalRate
  minSurvivalRateSe               Decimal? @map("min_survival_rate_se") @db.Decimal
  minSurvivalRateLowCI            Decimal? @map("min_survival_rate_low_ci") @db.Decimal
  minSurvivalRateHighCI           Decimal? @map("min_survival_rate_high_ci") @db.Decimal
  minSurvivalRateDataQuality      String?  @map("min_survival_rate_data_quality") @db.VarChar(255)

  // Données de confiance pour meanSurvivalRate
  meanSurvivalRateSe              Decimal? @map("mean_survival_rate_se") @db.Decimal
  meanSurvivalRateLowCI           Decimal? @map("mean_survival_rate_low_ci") @db.Decimal
  meanSurvivalRateHighCI          Decimal? @map("mean_survival_rate_high_ci") @db.Decimal
  meanSurvivalRateDataQuality     String?  @map("mean_survival_rate_data_quality") @db.VarChar(255)

  // Données de confiance pour maxSurvivalRate
  maxSurvivalRateSe               Decimal? @map("max_survival_rate_se") @db.Decimal
  maxSurvivalRateLowCI            Decimal? @map("max_survival_rate_low_ci") @db.Decimal
  maxSurvivalRateHighCI           Decimal? @map("max_survival_rate_high_ci") @db.Decimal
  maxSurvivalRateDataQuality      String?  @map("max_survival_rate_data_quality") @db.VarChar(255)

  // Données de confiance pour recaptureProbability
  recaptureProbabilitySe          Decimal? @map("recapture_probability_se") @db.Decimal
  recaptureProbabilityLowCI       Decimal? @map("recapture_probability_low_ci") @db.Decimal
  recaptureProbabilityHighCI      Decimal? @map("recapture_probability_high_ci") @db.Decimal
  recaptureProbabilityDataQuality String?  @map("recapture_probability_data_quality") @db.VarChar(255)

  // Données de confiance pour minLongevityYears
  minLongevityYearsSe             Decimal? @map("min_longevity_years_se") @db.Decimal
  minLongevityYearsLowCI          Decimal? @map("min_longevity_years_low_ci") @db.Decimal
  minLongevityYearsHighCI         Decimal? @map("min_longevity_years_high_ci") @db.Decimal
  minLongevityYearsDataQuality    String?  @map("min_longevity_years_data_quality") @db.VarChar(255)

  // Données de confiance pour meanLongevityYears
  meanLongevityYearsSe            Decimal? @map("mean_longevity_years_se") @db.Decimal
  meanLongevityYearsLowCI         Decimal? @map("mean_longevity_years_low_ci") @db.Decimal
  meanLongevityYearsHighCI        Decimal? @map("mean_longevity_years_high_ci") @db.Decimal
  meanLongevityYearsDataQuality   String?  @map("mean_longevity_years_data_quality") @db.VarChar(255)

  // Données de confiance pour maxLongevityYears
  maxLongevityYearsSe             Decimal? @map("max_longevity_years_se") @db.Decimal
  maxLongevityYearsLowCI          Decimal? @map("max_longevity_years_low_ci") @db.Decimal
  maxLongevityYearsHighCI         Decimal? @map("max_longevity_years_high_ci") @db.Decimal
  maxLongevityYearsDataQuality    String?  @map("max_longevity_years_data_quality") @db.VarChar(255)

  ageGroupId Int?      @map("age_group_id")
  ageGroupRef AgeGroup? @relation(fields: [ageGroupId], references: [id], onDelete: Cascade)

  @@map("survival")
}

model Fecundity {
  id                              Int      @id @default(autoincrement())
  ageGroup                        String?  @map("age_group") @db.VarChar(255)
  ageGroupConsideredInPublication String?  @map("age_group_considered_in_the_publication") @db.VarChar(255)

  // Champs principaux
  minFecundityRate                Decimal? @map("min_fecundity_rate") @db.Decimal
  meanFecundityRate               Decimal? @map("mean_fecundity_rate") @db.Decimal
  maxFecundityRate                Decimal? @map("max_fecundity_rate") @db.Decimal
  minProductivity                 Decimal? @map("min_productivity") @db.Decimal
  meanProductivity                Decimal? @map("mean_productivity") @db.Decimal
  maxProductivity                 Decimal? @map("max_productivity") @db.Decimal

  // Données de confiance pour minFecundityRate
  minFecundityRateSe              Decimal? @map("min_fecundity_rate_se") @db.Decimal
  minFecundityRateLowCI           Decimal? @map("min_fecundity_rate_low_ci") @db.Decimal
  minFecundityRateHighCI          Decimal? @map("min_fecundity_rate_high_ci") @db.Decimal
  minFecundityRateDataQuality     String?  @map("min_fecundity_rate_data_quality") @db.VarChar(255)

  // Données de confiance pour meanFecundityRate
  meanFecundityRateSe             Decimal? @map("mean_fecundity_rate_se") @db.Decimal
  meanFecundityRateLowCI          Decimal? @map("mean_fecundity_rate_low_ci") @db.Decimal
  meanFecundityRateHighCI         Decimal? @map("mean_fecundity_rate_high_ci") @db.Decimal
  meanFecundityRateDataQuality    String?  @map("mean_fecundity_rate_data_quality") @db.VarChar(255)

  // Données de confiance pour maxFecundityRate
  maxFecundityRateSe              Decimal? @map("max_fecundity_rate_se") @db.Decimal
  maxFecundityRateLowCI           Decimal? @map("max_fecundity_rate_low_ci") @db.Decimal
  maxFecundityRateHighCI          Decimal? @map("max_fecundity_rate_high_ci") @db.Decimal
  maxFecundityRateDataQuality     String?  @map("max_fecundity_rate_data_quality") @db.VarChar(255)

  // Données de confiance pour minProductivity
  minProductivitySe               Decimal? @map("min_productivity_se") @db.Decimal
  minProductivityLowCI            Decimal? @map("min_productivity_low_ci") @db.Decimal
  minProductivityHighCI           Decimal? @map("min_productivity_high_ci") @db.Decimal
  minProductivityDataQuality      String?  @map("min_productivity_data_quality") @db.VarChar(255)

  // Données de confiance pour meanProductivity
  meanProductivitySe              Decimal? @map("mean_productivity_se") @db.Decimal
  meanProductivityLowCI           Decimal? @map("mean_productivity_low_ci") @db.Decimal
  meanProductivityHighCI          Decimal? @map("mean_productivity_high_ci") @db.Decimal
  meanProductivityDataQuality     String?  @map("mean_productivity_data_quality") @db.VarChar(255)

  // Données de confiance pour maxProductivity
  maxProductivitySe               Decimal? @map("max_productivity_se") @db.Decimal
  maxProductivityLowCI            Decimal? @map("max_productivity_low_ci") @db.Decimal
  maxProductivityHighCI           Decimal? @map("max_productivity_high_ci") @db.Decimal
  maxProductivityDataQuality      String?  @map("max_productivity_data_quality") @db.VarChar(255)

  ageGroupId Int?      @map("age_group_id")
  ageGroupRef AgeGroup? @relation(fields: [ageGroupId], references: [id], onDelete: Cascade)

  @@map("fecundity")
}

// ============================================
// AUTRES DONNÉES LIÉES AUX ESPÈCES
// ============================================

model DispersalDistance {
  id                    Int      @id @default(autoincrement())

  // Champs principaux
  meanDispersalDistance Decimal? @map("mean_dispersal_distance") @db.Decimal
  minDispersalDistance  Decimal? @map("min_dispersal_distance") @db.Decimal
  maxDispersalDistance  Decimal? @map("max_dispersal_distance") @db.Decimal

  // Données de confiance pour meanDispersalDistance
  meanDispersalDistanceSe           Decimal? @map("mean_dispersal_distance_se") @db.Decimal
  meanDispersalDistanceLowCI        Decimal? @map("mean_dispersal_distance_low_ci") @db.Decimal
  meanDispersalDistanceHighCI       Decimal? @map("mean_dispersal_distance_high_ci") @db.Decimal
  meanDispersalDistanceDataQuality  String?  @map("mean_dispersal_distance_data_quality") @db.VarChar(255)

  // Données de confiance pour minDispersalDistance
  minDispersalDistanceSe            Decimal? @map("min_dispersal_distance_se") @db.Decimal
  minDispersalDistanceLowCI         Decimal? @map("min_dispersal_distance_low_ci") @db.Decimal
  minDispersalDistanceHighCI        Decimal? @map("min_dispersal_distance_high_ci") @db.Decimal
  minDispersalDistanceDataQuality   String?  @map("min_dispersal_distance_data_quality") @db.VarChar(255)

  // Données de confiance pour maxDispersalDistance
  maxDispersalDistanceSe            Decimal? @map("max_dispersal_distance_se") @db.Decimal
  maxDispersalDistanceLowCI         Decimal? @map("max_dispersal_distance_low_ci") @db.Decimal
  maxDispersalDistanceHighCI        Decimal? @map("max_dispersal_distance_high_ci") @db.Decimal
  maxDispersalDistanceDataQuality   String?  @map("max_dispersal_distance_data_quality") @db.VarChar(255)

  /*
  Lié à BibliographySpecies
  Chaque combinaison Bibliography+Species peut avoir des dispersal distances
  */
  bibliographySpeciesId Int?                  @map("bibliography_species_id")
  bibliographySpecies   BibliographySpecies? @relation(fields: [bibliographySpeciesId], references: [id], onDelete: Cascade)

  @@map("dispersal_distance")
}

